
    local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/VNT-UNIVERSAL/MyCode/main/UILib"))()
    local w1 = library:Window("VNT Script - Misc")
    w1:Toggle(
        "Auto Lay Bang Gac",
        "BangGac",
        false,
        function(toggled)
            AutoLayBangGac = toggled
        end
    )
    w1:Button(
        "Turn On Shift Lock",
        function()
            game:GetService("Players").LocalPlayer.DevEnableMouseLock = true
        end
    )
    
    w1:Button(
        "Delete NameTag",
        function()
            game.Players.LocalPlayer:FindFirstChild('NameTag'):Destroy()
        end
    )
    w1:Button(
        "Hitbox",
        function()
            _G.HeadSize = 30
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
pcall(function()
v.Character.Hitbox.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.Hitbox.Transparency = 1
v.Character.Hitbox.BrickColor = BrickColor.new("Really black")
v.Character.Hitbox.Material = "Neon"
v.Character.Hitbox.CanCollide = false
end)
end
end
        end
    )
    w1:Button(
        "Reach",
        function()
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://413861777"
            sound.Parent = game:GetService("SoundService")
            sound:Play()
        
            wait()
            game.StarterGui:SetCore("SendNotification", {
                Title = "Need tool"; -- the title (ofc)
                Text = "If you dont have weapon then get one and use this command"; -- what the text says (ofc)
                Duration = 5; -- how long the notification should in secounds
            })
            for i,v in pairs(game:GetService'Players'.LocalPlayer.Character:GetChildren()) do
                if v:isA("Tool") then
                    local a = Instance.new("SelectionBox",v.Handle)
                    v.Handle.Massless = true
                    v.Handle.Transparency = 1
                    a.Adornee = v.Handle
                    v.Handle.Size = Vector3.new(5, 20 , 1)
                    local selectionBox = Instance.new("SelectionBox",v.Handle)
                    selectionBox.Adornee = v.Handle
                    selectionBox.Color3 = Color3.new(0, 0.313725, 0.47451)
                end
            end
        end
    )
    function Music()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://413861777"
        sound.Parent = game:GetService("SoundService")
        sound:Play()
    end
    w1:Button(
        "Unlock Car",
        function()
            if game.Players.LocalPlayer.Character.Humanoid.SeatPart then
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Key Unlock";
                    Text = "Waitings Key Unlock";
                    Duration = 5; 
                })
                print(game.Players.LocalPlayer.Character.Humanoid.SeatPart.Parent.EngineValue)
                if game.Players.LocalPlayer.Character.Humanoid.SeatPart.Parent.EngineValue then
                   game.Players.LocalPlayer.Character.Humanoid.SeatPart.Parent.EngineValue.Value = true
                    Music()
                    game.StarterGui:SetCore("SendNotification", {
                    Title = "key Unlock";
                    Text = "Success";
                    Duration = 5; 
                })
                else
                                game.StarterGui:SetCore("SendNotification", {
                    Title = "key Unlock";
                    Text = "Error";
                    Duration = 5; 
                })
                end
                else
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Key Unlock";
                    Text = "Sit to Car";
                    Duration = 5; 
                })
            end
        end
    )
    
    function GetBackpack(Weapon) 
        game:GetService("ReplicatedStorage").KnitPackages._Index:FindFirstChild("sleitnick_knit@1.7.0").knit.Services.InventoryService.RE.updateInventory:FireServer("refresh")
        game:GetService("ReplicatedStorage"):WaitForChild("KnitPackages")
            :WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.7.0")
            :WaitForChild("knit"):WaitForChild("Services")
            :WaitForChild("InventoryService"):WaitForChild("RE")
            :WaitForChild("updateInventory"):FireServer("eue", Weapon)
    end
    spawn(function()
     while wait(.3) do
       if AutoLayBangGac then
           if not game.Players.LocalPlayer.Backpack:FindFirstChild('băng gạc') then
           GetBackpack('băng gạc')
           end
       end
     end
    end)
    w1:Label("Shop")
    SoluongBangGac = 1
    w1:Slider(
        "Count :",
        "CpuntBangGac",
        1,
        99,
        function(value)
            SoluongBangGac = value
        end,
        1
    )
    w1:Button(
        "Buy BangGac",
        function()
            if type(SoluongBangGac) ~= 'number' then
                SoluongBangGac = 1
            end
            
            for i = 1, SoluongBangGac do
                game:GetService("ReplicatedStorage").KnitPackages._Index["sleitnick_knit@1.7.0"].knit.Services.ShopService.RE.buyItem:FireServer('băng gạc')
            end
        end
    )
    w1:Button(
        "Buy PhongLon",
        function()
            if not game.Players.LocalPlayer.Backpack:FindFirstChild('PhongLon') then
            game:GetService("ReplicatedStorage").KnitPackages._Index["sleitnick_knit@1.7.0"].knit.Services.ShopService.RE.buyItem:FireServer('PhongLon')
            end
        end
    )
    w1:Label("Made By ZeroTNgaming")
    local w1 = library:Window("VNT Script - Farm")
    w1:Label("Log")
    w1:Toggle(
        "Auto Farm Log",
        "FarmLog",
        false,
        function(toggled)
            AutoFarmLog = toggled
        end
    )
    
    function topos(Pos)
        Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
        if game.Players.LocalPlayer.Character.Humanoid.Sit == true then game.Players.LocalPlayer.Character.Humanoid.Sit = false end
        pcall(function() tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/210, Enum.EasingStyle.Linear),{CFrame = Pos}) end)
        tween:Play()
        if Distance <= 10 then
        tween:Cancel()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
        end
    end
    function tp(cf)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cf
    end
    function Tigger(proximityPrompt)
        local rootPart = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
        if proximityPrompt then
            pcall(function()
                rootPart.CFrame = proximityPrompt.Parent.CFrame
                proximityPrompt.Enabled = true
                proximityPrompt.HoldDuration = 0 
                fireproximityprompt(proximityPrompt, 1)
            end)
        else
            warn("ProximityPrompt is nil")
        end
    end
    function SellPart()
        local Npc = workspace.Jobs.Trees:FindFirstChild("Sell")
        if Npc then
            Tigger(Npc:FindFirstChildOfClass('ProximityPrompt') or Npc:FindFirstChild('ProximityPrompt'))
        else
            tp(CFrame.new(1966, 19, 149))
        end
    end
    function JoinTeam()
        local Npc = workspace.NPCs:FindFirstChild("Khai thác gỗ")
        if Npc then
            Tigger(Npc:FindFirstChildOfClass('ProximityPrompt') or Npc:FindFirstChild('ProximityPrompt'))
        else
            tp(CFrame.new(1959, 20, 126))
        end
    end
    function GetLog()
        local player = game.Players.LocalPlayer
        local gui = player:WaitForChild("PlayerGui")
        local topbar = gui:WaitForChild("TopbarStandard")
        local holders = topbar:WaitForChild("Holders")
        local left = holders:WaitForChild("Left")
        local iconButton = left:WaitForChild('logCount'):WaitForChild("IconButton")
        local menu = iconButton:WaitForChild("Menu")
        local iconSpot = menu:WaitForChild("IconSpot")
        local contents = iconSpot:WaitForChild("Contents")
        local iconLabelContainer = contents:WaitForChild("IconLabelContainer")
        local iconLabel = iconLabelContainer:WaitForChild("IconLabel")
    
        return tonumber(iconLabel.Text:gsub("%D", "")) or 0
    end
    
    
    function checklog(vc)
        return vc.LogClones:FindFirstChild('Log1') or vc.LogClones:FindFirstChild('Log2')
    end
    function CheckLogFree()
        for _, v in pairs(game.Workspace.Jobs.Trees:GetChildren()) do
            if v.Name == 'Tree' and checklog(v) then 
               return v
            end
        end
    end
    function EndLog(v)
        return v:FindFirstChild('Trunk').Transparency == 1 and not checklog(v)
    end
    function SkillDown(va)
        game:GetService("VirtualInputManager"):SendKeyEvent(true, va, false, game)
    end
    
    function SkillUp(va)
        game:GetService("VirtualInputManager"):SendKeyEvent(false, va, false, game)
    end
    function EquipWeapon(ToolSe)
        pcall(function()
        if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
            local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
            wait(.4)
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
            wait(.3)
            if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
            tool.Parent = game.Players.LocalPlayer.Character
            end
        end  
       end)
    end   
    function CheckItem(itemName,cplr)
        if cplr then
            local item = game.Players.LocalPlayer.Character:FindFirstChild(itemName)
            return item
        end
        local item = game.Players.LocalPlayer.Backpack:FindFirstChild(itemName) or game.Players.LocalPlayer.Character:FindFirstChild(itemName)
        if item then
            return item
        end
    end
    function CheckTeam(teamName)
        local team = game.Players.LocalPlayer.Team.Name
        if team == teamName then
            return team
        end
    end
    spawn(function()
        while wait() do
           if AutoFarmLog then
            if CheckTeam('Khai thác gỗ') then
               if CheckItem('Rìu') then
                    if not CheckItem('Rìu',true) then
                        EquipWeapon('Rìu')
                    end
                        for _, v in pairs(game.Workspace.Jobs.Trees:GetChildren()) do
                        if v.Name == 'Tree' and v.Trunk.Transparency ~= 1 then
                            repeat wait()
                                pcall(function()
                                if CheckLogFree() then
                                    Tigger(checklog(CheckLogFree()).Trunk.Collect)
                                else
                                task.wait()
                                if checklog(v) then
                                    Tigger(checklog(v).Trunk.Collect)
                                else    
                                    Tigger(v.Prompt.CutWood)
                                end
                            end
                        end)
                            until not AutoFarmLog or EndLog(v)
                        end
                    end
                else
                    wait(.3)
                    if not CheckItem('Rìu') then
                    GetBackpack('Rìu') 
                    end
                end
            else
                JoinTeam()
            end
           end
        end
    end)
    
    w1:Label("Made By z4rx")
    
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
    
